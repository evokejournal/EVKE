{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": false,
    ".write": false,
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        "insights": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "preferences": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "entries": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid",
          ".indexOn": ["timestamp"]
        }
      }
    }
  }
}